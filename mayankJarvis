#include<stdio.h>
#include<conio.h>
void Langford(int arr[8],int n,int n1)
{ if(n==0){
	for ( int i=0;i<n1;i++)
	{ printf("%c",arr[i]+64);
	}
	printf("\n");
	return;
}
for(int i=0;i<n1-n-1;i++)
{
	if(arr[i]==0 && arr[i+n+1]==0)
	{
		arr[i]=n;arr[i+n+1]=n;
		Langford(arr,n-1,n1);
		arr[i]=0;arr[i+n+1]=0;
	}
}
}
int main()
{ int n;
printf("enter the no.");
scanf("%d",&n);
int n1=2*n;
int arr[n1];
for(int i=0;i<(2*n);i++)
arr[i]=0;
if(n%4!=0&&(n+1)%4!=0)
printf("not possible langforg pairing");
else
{
Langford(arr,n,n1);
}
}
